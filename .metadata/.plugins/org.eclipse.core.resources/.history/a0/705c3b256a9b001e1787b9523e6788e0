package com.learning;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class ShapesTest {

	Shapes shapes = new Shapes();
	@Test
	void areaSquareTest() 
	{
		assertEquals(25,shapes.areaSquare(5));
	}
	@Test
	void areaCircleTest() 
	{
		assertEquals(28.25,shapes.areaCircle(3));
	}

}
